pscoast -R0/360/-90/90 -JG-45/70/10 -Dc -A1000 -G200 -P -X1 -Y1 -B15g15 -K >map.ps
psxy IN.ll -R0/360/-90/90 -JG-45/70/10  -G255/0/255  -O -K -Sc.2 -: >>map.ps
psxy AF.ll -R0/360/-90/90 -JG-45/70/10  -G0/255/0  -O -K -Sc.2 -: >>map.ps
psxy AU.ll -R0/360/-90/90 -JG-45/70/10  -G0/0/255  -O -K -Sc.2 -:  >>map.ps
psxy ANT.ll -R0/360/-90/90 -JG-45/70/10  -G255 -O -K -Sc.2 -: >>map.ps
psxy SAM.ll -R0/360/-90/90 -JG-45/70/10  -G255/255/0 -O -Sc.2 -: >>map.ps
