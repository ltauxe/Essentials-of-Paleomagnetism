()	"Mask Area",mask_area()Endmenu "macros"endmenu "FORCinel"	"Load FORCs",Load_FORCs()//	"Load Josh's FORCs", Load_Josh_FORCs()	"-"	"Display FORCs",Display_FORCs()//	"Display Josh's FORCs",Display_Josh_FORCs()	"-"	"Process FORCs",Process_FORCs(1)//	"Process Josh's FORCs",Process_Josh_FORCs(1)	"-"	"Change Smoothing Factor",Change_smoothing_factor(1)	"Remove all Smoothing",calc_forc_no_smooth()//	"Change Robust Smoothing Factor",Change_Robust_Smoothing_Factor(1)	"-"	"Display Processed FORCs/d",Display_Processed_FORCs()	"Add Contours", Add_Contours()	"Add Colour Scale",add_colour_scale()	"-"	"Autoscale Intensity Range/1",Autoscale_Intensity_Range()	"Autoscale Contour Range/2",Autoscale_Contour_Range()	"Reset to Default Intensity Range/3",Reset_Intensity_Scale()	"Reset to Default Contour Range/4",Reset_Contour_Scale()	"Convert to Greyscale",Black_and_White()	"-"	"Coercivity Profile",hc_profile()	"Vertical Profile",hu_profile()	"Averaged Vertical Profile",hu_profile_average()	"Marginal Coercivity Distribution",projected_hc_profile()	"Normalise FORC Distribution",normalised_forc_distribution()	"-"	"Drift Measurements",Display_Drift()	"Reversible Ridge",Display_Reversible_Ridge()	"-"	"Data Masking Panel...",data_masking()	"Mask Area/5",mask_area()	"-"	"Calculate Optimum Smoothing Factor",Calc_Optimum_Smoothing()endmacro Load_FORCs()	Load_FORC_Data()	Display_FORCs()endmacro Load_Josh_FORCs()	Load_FORC_Data_Old()	Display_Josh_FORCs()endfunction Load_FORC_Data_Old()	silent 1	SetDataFolder root:	variable fileref	variable/g ncurves	variable t1,t2,t3,t4,t5,t6	string temp,wavename	open/r/d fileref	print s_filename	open/r/T="????"  fileref as s_filename	variable i=0,wavenamelength	loadwave/q/a/g/d/l={0,1,0,0,2} s_filename	if (datafolderexists(s_filename))		killdatafolder :$s_filename	endif	newdatafolder/o/s root:$s_filename	do 		wavename=stringfromlist(i,s_wavenames,";")		wavenamelength=strlen(wavename)		if (wavenamelength>0)			movewave root:$wavename,:		endif		i+=1	while (wavenamelength>0)//	 Read No of curves		freadline fileref,temp	sscanf temp,"%f,%f,%f,%f,%f,%f,%f",t1,t2,t3,t4,t5,t6,ncurves 	close fileref	make/o/n=1 ncrvs	ncrvs=ncurvesendfunction Load_FORC_Data()	silent 1	SetDataFolder root:	variable fileref	variable/g ncurves,hb1,hb2,hc2	string temp,wavename,wavename1,wavename2,unit	variable value1,value2	open/r/d/T="????"  fileref	print s_filename	open/r fileref as s_filename	variable i=0,j=1,wavenamelength	// Read units	do		freadline fileref,temp	while (strsearch(temp,"Units of measure:",0)==-1)	sscanf temp,"Units of measure: %s",unit print unit	// Read hb1	do		freadline fileref,temp	while (strsearch(temp,"Hb1",0)==-1)	sscanf temp,"Hb1 = %f",hb1 	// Read hb2	do		freadline fileref,temp	while (strsearch(temp,"Hb2",0)==-1)	sscanf temp,"Hb2 = %f",hb2 	// Read hc2	do		freadline fileref,temp	while (strsearch(temp,"Hc2",0)==-1)	sscanf temp,"Hc2 = %f",hc2 	// Read No of curves	do		freadline fileref,temp	while (strsearch(temp,"NCrv",0)==-1)	sscanf temp,"NCrv = %f",ncurves 	do		freadline fileref,temp	while (strsearch(temp,"NData",0)==-1)			variable k=0	make/o/n=(ncurves) driftfield,driftmag	wave df=driftfield,dm=driftmag	do		do			freadline fileref,temp		while (strsearch(temp,"\r",0)==0)			if (strlen(temp)==0)				break			endif			sscanf temp,"%f,%f", value1,value2			df[k]=value1			dm[k]=value2			wavename1="wave"+num2str(i)			wavename2="wave"+num2str(i+1)			make/o/n=0 $wavename1,$wavename2			wave w1=$wavename1,w2=$wavename2		do			freadline fileref,temp		while (strsearch(temp,"\r",0)==0)		do			redimension/n=(j) w1,w2			sscanf temp,"%f,%f", value1,value2			w1[j]=value1			w2[j]=value2			j+=1			freadline fileref,temp			if (strlen(temp)==0)				break			endif		while (cmpstr(temp,"\r")!=0)			j=1			i+=2			k+=1		while (1)	killwaves w1,w2	i = itemsinlist(s_filename,":")-1	s_filename= stringfromlist(i,s_filename,":")	i=0	if (datafolderexists(s_filename))		killdatafolder :$s_filename	endif	string s_wavenames = WaveList("*",";","")	newdatafolder/o/s root:$s_filename	do 		wavename=stringfromlist(i,s_wavenames,";")		wavenamelength=strlen(wavename)		if (wavenamelength>0)			movewave root:$wavename,:		endif		i+=1	while (wavenamelength>0)	string/g units=unit	close fileref	make/o/n=1 ncrvs,b1,b2,c2	ncrvs=ncurves	b1=hb1	b2=hb2	c2=hc2endfunction forc(w,p,q,s)	wave w	variable p,q,s	wave w_coef	if (numtype(w[p][q])==0)	CurveFit/q/X=1 poly2D 2,  w[(p-s),(p+s)][(q-s),(q+s)] 		return w_coef[4]	else	return nan	endifendmacro Display_FORCs()	variable n = ncrvs[0]*2-1	display_forc_data(n)endmacro Display_Josh_FORCs()	variable n = ncrvs[0]*4-1	display_forc_data_josh(n)endfunction display_forc_data(no)	variable no	variable i	display wave1 vs wave0	string wavename1,wavename2	svar un=units	for (i=2;i<=no;i+=2)		wavename1="wave"+num2str(i)		wavename2="wave"+num2str(i+1)		appendtograph $wavename2 vs $wavename1	endfor	ModifyGraph tick=2,zero=3,mirror=1,standoff=0	Label left "Moment"	if (cmpstr(un,"cgs")==0)		Label bottom "Field (Oe)"	else		Label bottom "Field (T)"	endifendfunction display_forc_data_josh(no)	variable no	variable i	display wave3 vs wave2	string wavename1,wavename2	for (i=6;i<=no;i+=4)		wavename1="wave"+num2str(i)		wavename2="wave"+num2str(i+1)		appendtograph $wavename2 vs $wavename1	endfor	ModifyGraph zero=1	Label bottom "Field (Oe)"	Label left "Moment (emu)"endmacro Process_FORCs(sf)	variable sf	variable n=ncrvs[0]	Process_FORC_Data(n,sf)	Display_Processed_FORCs()endfunction Process_FORC_Data(no,sf)	variable no,sf	variable i,j=0	variable nx = 1 + (no-1)*2	string wavename1	make/o/n=(nx,no) matrix1,matrix2	make/o/n=(no) ha	make/o/n=(nx) hb,temp	matrix1=nan	matrix2=nan	for (i=no*2-2;i>-1;i-=2)		wavename1="wave"+num2str(i)		wave wavname=$wavename1		nx=numpnts(wavname)		matrix1[j,(j+nx-1)][i/2]=wavname[p-j]		j+=1	endfor	j=0	for (i=no*2-1;i>-1;i-=2)		wavename1="wave"+num2str(i)		wave wavname=$wavename1		nx=numpnts(wavname)		matrix2[j,(j+nx-1)][i/2]=wavname[p-j]		j+=1	endfor	j=0	for (i=no*2-2;i>-1;i-=2)		wavename1="wave"+num2str(i)		wave wavname=$wavename1		ha[i/2]=wavname[0]	endfor	nx = 1 + (no-1)*2	for (i=0;i<nx;i+=1)		temp=matrix1[i][p]		wavestats/q temp		hb[i]=v_avg	endforduplicate/o matrix2 hamatrix,hbmatrix,hcmatrix,humatrixhamatrix[0,nx][]=ha[q]hbmatrix[][0,no]=hb[p]hcmatrix=(hbmatrix-hamatrix)/2humatrix=(hamatrix+hbmatrix)/2imagerotate/a=-45 hcmatrixrename m_rotatedimage hc_rotatedimagerotate/a=-45 humatrixrename m_rotatedimage hu_rotatedwave hc_rotatedwave hu_rotatedvariable nrow=dimsize(hc_rotated,0)variable ncol=dimsize(hc_rotated,1)make/o/n=(nrow) hcmake/o/n=(ncol) huredimension/n=(ncol) tempfor (i=0;i<nrow;i+=1)	temp=hc_rotated[i][p]	wavestats/q temp	hc[i]=v_avgendforredimension/n=(nrow) tempfor (i=0;i<ncol;i+=1)	temp=hu_rotated[p][i]	wavestats/q temp	hu[i]=v_avgendforduplicate/o hc hc_plus1redimension/n=(nrow+1) hc_plus1duplicate/o hu hu_plus1redimension/n=(ncol+1) hu_plus1CurveFit/q line  hc /D wave w_coefhc=numtype(hc[p])==2?  W_coef[0]+W_coef[1]*p : hc[p]hc_plus1=p==nrow?  W_coef[0]+W_coef[1]*p : hc[p]CurveFit/q line  hu /D hu=numtype(hu[p])==2?  W_coef[0]+W_coef[1]*p : hu[p]hu_plus1=p==ncol?  W_coef[0]+W_coef[1]*p : hu[p]imageinterpolate/f={4,4} bilinear hc_rotatedrename m_interpolatedimage hc_rotated_interpimageinterpolate/f={4,4} bilinear hu_rotatedrename m_interpolatedimage hu_rotated_interpwave hc_rotated_interp,hu_rotated_interpnrow=dimsize(hc_rotated_interp,0)ncol=dimsize(hc_rotated_interp,1)make/o/n=(nrow) hc_interpmake/o/n=(ncol) hu_interpredimension/n=(ncol) tempfor (i=0;i<nrow;i+=1)	temp=hc_rotated_interp[i][p]	wavestats/q temp	hc_interp[i]=v_avgendforredimension/n=(nrow) tempfor (i=0;i<ncol;i+=1)	temp=hu_rotated_interp[p][i]	wavestats/q temp	hu_interp[i]=v_avgendforduplicate/o hc_interp hc_interp_plus1redimension/n=(nrow+1) hc_interp_plus1duplicate/o hu_interp hu_interp_plus1redimension/n=(ncol+1) hu_interp_plus1CurveFit/q line  hc_interp /D wave w_coefhc_interp=numtype(hc_interp[p])==2?  W_coef[0]+W_coef[1]*p : hc_interp[p]hc_interp_plus1=p==nrow?  W_coef[0]+W_coef[1]*p : hc_interp[p]CurveFit/q line  hu_interp /D hu_interp=numtype(hu_interp[p])==2?  W_coef[0]+W_coef[1]*p : hu_interp[p]hu_interp_plus1=p==ncol?  W_coef[0]+W_coef[1]*p : hu_interp[p]duplicate/o matrix2 matrix4make/o/n=1/t processed_forc_win_nameChange_Smoothing_Factor(sf)endmacro Process_Josh_FORCs(sf)	variable sf	variable n=ncrvs[0]	Process_AGM_Data(n,sf)	Display_Processed_FORCs()endfunction Process_AGM_Data(no,sf)	variable no,sf	variable i,j=0	variable nx = 1 + (no-1)*2	string wavename1	make/o/n=(nx,no) matrix1,matrix2	make/o/n=(no) ha	make/o/n=(nx) hb,temp	matrix1=nan	matrix2=nan	for (i=no*4-2;i>-1;i-=4)		wavename1="wave"+num2str(i)		wave wavname=$wavename1		nx=numpnts(wavname)		matrix1[j,(j+nx-1)][i/4 ]=wavname[p-j]		matrix1[j][i/4 ]=nan		j+=1	endfor	j=0	for (i=no*4-1;i>-1;i-=4)		wavename1="wave"+num2str(i)		wave wavname=$wavename1		nx=numpnts(wavname)		matrix2[j,(j+nx-1)][i/4]=wavname[p-j]		matrix2[j][i/4 ]=nan		j+=1	endfor	j=0	for (i=no*4-2;i>-1;i-=4)		wavename1="wave"+num2str(i)		wave wavname=$wavename1		ha[i/4]=wavname[0]	endfor	nx = 1 + (no-1)*2	for (i=0;i<nx;i+=1)		temp=matrix1[i][p]		wavestats/q temp		hb[i]=v_avg	endforduplicate/o matrix2 hamatrix,hbmatrix,hcmatrix,humatrixhamatrix[0,nx][]=ha[q]hbmatrix[][0,no]=hb[p]hcmatrix=(hbmatrix-hamatrix)/2humatrix=(hamatrix+hbmatrix)/2imagerotate/a=-45 hcmatrixrename m_rotatedimage hc_rotatedimagerotate/a=-45 humatrixrename m_rotatedimage hu_rotatedwave hc_rotatedwave hu_rotatedvariable nrow=dimsize(hc_rotated,0)variable ncol=dimsize(hc_rotated,1)make/o/n=(nrow) hcmake/o/n=(ncol) huredimension/n=(ncol) tempfor (i=0;i<nrow;i+=1)	temp=hc_rotated[i][p]	wavestats/q temp	hc[i]=v_avgendforredimension/n=(nrow) tempfor (i=0;i<ncol;i+=1)	temp=hu_rotated[p][i]	wavestats/q temp	hu[i]=v_avgendforduplicate/o hc hc_plus1redimension/n=(nrow+1) hc_plus1duplicate/o hu hu_plus1redimension/n=(ncol+1) hu_plus1CurveFit/q line  hc /D wave w_coefhc=numtype(hc[p])==2?  W_coef[0]+W_coef[1]*p : hc[p]hc_plus1=p==nrow?  W_coef[0]+W_coef[1]*p : hc[p]CurveFit/q line  hu /D hu=numtype(hu[p])==2?  W_coef[0]+W_coef[1]*p : hu[p]hu_plus1=p==ncol?  W_coef[0]+W_coef[1]*p : hu[p]imageinterpolate/f={4,4} bilinear hc_rotatedrename m_interpolatedimage hc_rotated_interpimageinterpolate/f={4,4} bilinear hu_rotatedrename m_interpolatedimage hu_rotated_interpwave hc_rotated_interp,hu_rotated_interpnrow=dimsize(hc_rotated_interp,0)ncol=dimsize(hc_rotated_interp,1)make/o/n=(nrow) hc_interpmake/o/n=(ncol) hu_interpredimension/n=(ncol) tempfor (i=0;i<nrow;i+=1)	temp=hc_rotated_interp[i][p]	wavestats/q temp	hc_interp[i]=v_avgendforredimension/n=(nrow) tempfor (i=0;i<ncol;i+=1)	temp=hu_rotated_interp[p][i]	wavestats/q temp	hu_interp[i]=v_avgendforduplicate/o hc_interp hc_interp_plus1redimension/n=(nrow+1) hc_interp_plus1duplicate/o hu_interp hu_interp_plus1redimension/n=(ncol+1) hu_interp_plus1CurveFit/q line  hc_interp /D wave w_coefhc_interp=numtype(hc_interp[p])==2?  W_coef[0]+W_coef[1]*p : hc_interp[p]hc_interp_plus1=p==nrow?  W_coef[0]+W_coef[1]*p : hc_interp[p]CurveFit/q line  hu_interp /D hu_interp=numtype(hu_interp[p])==2?  W_coef[0]+W_coef[1]*p : hu_interp[p]hu_interp_plus1=p==ncol?  W_coef[0]+W_coef[1]*p : hu_interp[p]duplicate matrix2 matrix4Change_Smoothing_Factor(sf)endfunction Process_FORCface_Data(no,sf)	variable no,sf	variable i,j=0	variable nx = 1 + (no-1)*2	string wavename1	wave matrix1,matrix2	make/o/n=(no) ha	make/o/n=(nx) hb,temp	j=0	for (i=no;i>0;i-=1)		ha[i-1]=matrix1[no-i][i-1]	endfor	nx = 1 + (no-1)*2	for (i=0;i<nx;i+=1)		temp=matrix1[i][p]		wavestats/q temp		hb[i]=v_avg	endforduplicate/o matrix2 hamatrix,hbmatrix,hcmatrix,humatrixhamatrix[0,nx][]=ha[q]hbmatrix[][0,no]=hb[p]hcmatrix=(hbmatrix-hamatrix)/2humatrix=(hamatrix+hbmatrix)/2imagerotate/a=-45 hcmatrixrename m_rotatedimage hc_rotatedimagerotate/a=-45 humatrixrename m_rotatedimage hu_rotatedwave hc_rotatedwave hu_rotatedvariable nrow=dimsize(hc_rotated,0)variable ncol=dimsize(hc_rotated,1)make/o/n=(nrow) hcmake/o/n=(ncol) huredimension/n=(ncol) tempfor (i=0;i<nrow;i+=1)	temp=hc_rotated[i][p]	wavestats/q temp	hc[i]=v_avgendforredimension/n=(nrow) tempfor (i=0;i<ncol;i+=1)	temp=hu_rotated[p][i]	wavestats/q temp	hu[i]=v_avgendforduplicate/o hc hc_plus1redimension/n=(nrow+1) hc_plus1duplicate/o hu hu_plus1redimension/n=(ncol+1) hu_plus1CurveFit/q line  hc /D wave w_coefhc=numtype(hc[p])==2?  W_coef[0]+W_coef[1]*p : hc[p]hc_plus1=p==nrow?  W_coef[0]+W_coef[1]*p : hc[p]CurveFit/q line  hu /D hu=numtype(hu[p])==2?  W_coef[0]+W_coef[1]*p : hu[p]hu_plus1=p==ncol?  W_coef[0]+W_coef[1]*p : hu[p]imageinterpolate/f={4,4} bilinear hc_rotatedrename m_interpolatedimage hc_rotated_interpimageinterpolate/f={4,4} bilinear hu_rotatedrename m_interpolatedimage hu_rotated_interpwave hc_rotated_interp,hu_rotated_interpnrow=dimsize(hc_rotated_interp,0)ncol=dimsize(hc_rotated_interp,1)make/o/n=(nrow) hc_interpmake/o/n=(ncol) hu_interpredimension/n=(ncol) tempfor (i=0;i<nrow;i+=1)	temp=hc_rotated_interp[i][p]	wavestats/q temp	hc_interp[i]=v_avgendforredimension/n=(nrow) tempfor (i=0;i<ncol;i+=1)	temp=hu_rotated_interp[p][i]	wavestats/q temp	hu_interp[i]=v_avgendforduplicate/o hc_interp hc_interp_plus1redimension/n=(nrow+1) hc_interp_plus1duplicate/o hu_interp hu_interp_plus1redimension/n=(ncol+1) hu_interp_plus1CurveFit/q line  hc_interp /D wave w_coefhc_interp=numtype(hc_interp[p])==2?  W_coef[0]+W_coef[1]*p : hc_interp[p]hc_interp_plus1=p==nrow?  W_coef[0]+W_coef[1]*p : hc_interp[p]CurveFit/q line  hu_interp /D hu_interp=numtype(hu_interp[p])==2?  W_coef[0]+W_coef[1]*p : hu_interp[p]hu_interp_plus1=p==ncol?  W_coef[0]+W_coef[1]*p : hu_interp[p]duplicate/o matrix2 matrix4Change_Smoothing_Factor(sf)endmacro Display_Processed_FORCs()	pauseupdate	string windowname=getdatafolder(0)+" with SF = "+num2str(last_sf_used[0])	Display as windowname;AppendImage M_InterpolatedImage vs {hc_interp_plus1,hu_interp_plus1}	ModifyImage M_InterpolatedImage ctab= {*,*,Rainbow,1}	ModifyGraph width={Plan,1,bottom,left},height=0	ModifyGraph zero(left)=3	make/o/n=1/t processed_forc_win_name=WinName(0,1)	if (cmpstr(units,"cgs")==0)	Label left "Hu (Oe)";DelayUpdate	Label bottom "Hc (Oe)"	else	Label left "Hu (T)";DelayUpdate	Label bottom "Hc (T)"	endif	duplicate/o matrix2 roimask	redimension/b/u roimask	roimask=numtype(matrix2[p][q])==2? 1 : 0	imagestats/r=roimask matrix4	ModifyImage m_interpolatedimage ctab= {v_min,v_max,Rainbow,1}if (exists("c2")==1)	SetAxis bottom 0,(c2[0])	SetAxis left (b1[0]),(b2[0])endif	resumeupdateendmacromacro Black_and_White()	pauseupdate	duplicate/o matrix2 roimask	redimension/b/u roimask	roimask=numtype(matrix2[p][q])==2? 1 : 0	imagestats/r=roimask matrix4	ModifyImage m_interpolatedimage ctab= {v_min,v_max,grays,0}	resumeupdateendmacromacro Reset_Intensity_Scale()	string winname=getdatafolder(0)	duplicate/o matrix2 roimask	redimension/b/u roimask	roimask=numtype(matrix2[p][q])==2? 1 : 0	imagestats/r=roimask matrix4	ModifyImage m_interpolatedimage ctab= {v_min,v_max,Rainbow,1}endmacromacro Reset_Contour_Scale()	string winname=getdatafolder(0)	duplicate/o matrix2 roimask	redimension/b/u roimask	roimask=numtype(matrix2[p][q])==2? 1 : 0	imagestats/r=roimask matrix4	ModifyContour M_RotatedImage autoLevels={v_min,v_max,11}endmacrofunction projected_hc_profile()	wave c2,b1,b2,m_interpolatedimage	findlevel/q/p hc_interp_plus1,0	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,c2[0]	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b1[0]	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b2[0]	variable to=floor(v_levelx)		variable psize=numpnts(hc_interp_plus1)	make/o/n=(psize) marginal_profile	make/o/n=(bo-to) temp_profile	variable i	for (i=0;i<psize;i+=1)		temp_profile=m_interpolatedimage[i][to+p]		marginal_profile[i]=sum(temp_profile,-inf,inf)	endfor	checkdisplayed/a marginal_profile	if (v_flag==0)		display marginal_profile vs hc_interp_plus1		SetAxis bottom 0,c2[0]		SetAxis/a=2 left 		Label left "Marginal distribution";DelayUpdate		Label bottom "Hc (Oe)"		ModifyGraph rgb=(0,0,0)		ModifyGraph tick=2,mirror=1,standoff=0	endifendfunction normalised_forc_distribution()	wave c2,b1,b2,m_interpolatedimage	findlevel/q/p hc_interp_plus1,0	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,c2[0]	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b1[0]	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b2[0]	variable to=floor(v_levelx)		variable psize=numpnts(hc_interp_plus1)	make/o/n=(psize) profile	make/o/n=(bo-to) temp_profile	variable i	for (i=0;i<psize;i+=1)		temp_profile=m_interpolatedimage[i][to+p]		wavestats/q temp_profile		m_interpolatedimage[i][]/=v_max	endfor	wave/t processed_forc_win_name	string processed_winname=processed_forc_win_name[0]	checkdisplayed/w=$processed_winname m_interpolatedimage	if (v_flag>0)		ModifyImage/w=$processed_winname m_interpolatedimage ctab= {*,*,Rainbow,1}		ModifyImage/w=$processed_winname m_interpolatedimage ctabAutoscale=1,lookup= $""	endif	endfunction hc_profile()	variable hulevel	Prompt hulevel,"Enter Hu value for coercivity profile"	DoPrompt "Enter Hu value for coercivity profile",hulevel	wave c2,b1,b2,m_interpolatedimage	findlevel/q/p hc_interp_plus1,0	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,c2[0]	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b1[0]	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b2[0]	variable to=floor(v_levelx)	findlevel/q/p hu_interp_plus1,hulevel	variable i=floor(v_levelx)	variable psize=numpnts(hc_interp_plus1)	make/o/n=(psize) horizontal_profile	horizontal_profile=m_interpolatedimage[p][i]	checkdisplayed/a horizontal_profile	if (v_flag==0)	display horizontal_profile vs hc_interp_plus1	SetAxis bottom 0,c2[0]	SetAxis/a=2 left 	Label left "Coercivity profile";DelayUpdate	Label bottom "Hc (Oe)"	ModifyGraph rgb=(0,0,0)	ModifyGraph tick=2,mirror=1,standoff=0	endifendfunction hu_profile()	variable hclevel	Prompt hclevel,"Enter Hc value for vertical profile"	DoPrompt "Enter Hc value for vertical profile",hclevel	wave c2,b1,b2,m_interpolatedimage	findlevel/q/p hc_interp_plus1,0	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,c2[0]	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b1[0]	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b2[0]	variable to=floor(v_levelx)	findlevel/q/p hc_interp_plus1,hclevel	variable i=floor(v_levelx)	variable psize=numpnts(hu_interp_plus1)	make/o/n=(psize) vertical_profile	vertical_profile=m_interpolatedimage[i][p]	checkdisplayed/a vertical_profile	if (v_flag==0)		display vertical_profile vs hu_interp_plus1		SetAxis bottom b1[0],b2[0]		SetAxis/a=2 left 		Label left "Vertical profile";DelayUpdate		Label bottom "Hu (Oe)"		ModifyGraph rgb=(0,0,0)		ModifyGraph tick=2,mirror=1,standoff=0	endifendfunction hu_profile_average()	variable hclevel1,hclevel2	Prompt hclevel1,"Enter starting Hc value for vertical profile average"	Prompt hclevel2,"Enter final Hc value for vertical profile average"	DoPrompt "Enter Hc value range for vertical profile",hclevel1,hclevel2	wave c2,b1,b2,m_interpolatedimage	findlevel/q/p hc_interp_plus1,0	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,c2[0]	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b1[0]	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,b2[0]	variable to=floor(v_levelx)	findlevel/q/p hc_interp_plus1,hclevel1	variable i=floor(v_levelx)	findlevel/q/p hc_interp_plus1,hclevel2	variable j=floor(v_levelx)	variable k	variable psize=numpnts(hu_interp_plus1)	make/o/n=(psize) average_profile	average_profile=0	for (k=i;k<=j;k+=1)		average_profile+=m_interpolatedimage[k][p]	endfor		average_profile/=j-i + 1	checkdisplayed/a average_profile	if (v_flag==0)		display average_profile vs hu_interp_plus1		SetAxis bottom b1[0],b2[0]		SetAxis/a=2 left 		Label left "Averaged vertical profile";DelayUpdate		Label bottom "Hu (Oe)"		ModifyGraph rgb=(0,0,0)		ModifyGraph tick=2,mirror=1,standoff=0	endifendmacro Add_Contours()	pauseupdate	AppendMatrixContour M_RotatedImage vs {hc,hu};DelayUpdate	ModifyContour M_RotatedImage rgbLines=(0,0,0),labels=0	duplicate/o matrix2 roimask	redimension/b/u roimask	roimask=numtype(matrix2[p][q])==2? 1 : 0	imagestats/r=roimask matrix4	ModifyContour M_RotatedImage autoLevels={v_min,v_max,11}	resumeupdateendmacro Autoscale_Intensity_Range()	getmarquee left,bottom	findlevel/q/p hc_interp_plus1,v_left	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,v_right	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,v_bottom	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,v_top	variable to=floor(v_levelx)	imagestats/g={l,r,to,bo} m_interpolatedimage	ModifyImage m_interpolatedimage ctab= {(v_min),(v_max),Rainbow,1}	endmacro Autoscale_Contour_Range()	getmarquee left,bottom	findlevel/q/p hc_interp_plus1,v_left	variable l=floor(v_levelx)	findlevel/q/p hc_interp_plus1,v_right	variable r=floor(v_levelx)	findlevel/q/p hu_interp_plus1,v_bottom	variable bo=floor(v_levelx)	findlevel/q/p hu_interp_plus1,v_top	variable to=floor(v_levelx)	imagestats/g={l,r,to,bo} m_interpolatedimage	ModifyContour M_RotatedImage autoLevels={v_min,v_max,11}endfunction Change_Smoothing_Factor(sf)	variable sf	if (sf==1)		prompt sf, "Enter new smoothing factor. Typically sf = 1-3"		DoPrompt "Smoothing Factor",sf	endif	make/o/n=1 last_sf_used	last_sf_used=sf	wave matrix2		wavestats/q matrix2	sf=(2*sf+1)^2/v_npnts	loess/z=1/ord=2/smth=(sf)/e=1/dest=matrix4 srcwave=matrix2//	loess/z=1/ord=2/N=((2*sf+1)^2 )/e=1/dest=matrix4 srcwave=matrix2	calc_reversible_ridge()	Differentiate/meth=0/DIM=0  matrix4	Differentiate/meth=0/DIM=1  matrix4	duplicate/o matrix4 matrix4_int	matrix4_int=numtype(matrix2[p][q])==2? 0 : matrix4[p][q]	Integrate/DIM=0 /METH=1 matrix4_int	Integrate/DIM=1 /METH=1 matrix4_INT	matrix4/=matrix4_int[dimsize(matrix4,0)-1][dimsize(matrix4,1)-1]	killwaves matrix4_int	imagerotate/a=-45 matrix4	imageinterpolate/f={4,4} bilinear m_rotatedimage				duplicate/o matrix2 roimask	redimension/b/u roimask	roimask=numtype(matrix2[p][q])==2? 1 : 0	imagestats/r=roimask matrix4	wave/t processed_forc_win_name	string processed_winname=processed_forc_win_name[0]	checkdisplayed/w=$processed_winname m_interpolatedimage	if (v_flag>0)		ModifyImage/w=$processed_winname m_interpolatedimage ctab= {v_min,v_max,Rainbow,1}		string windowname=getdatafolder(0)+" with SF = "+num2str(last_sf_used[0])		dowindow/t $processed_winname,windowname//		print processed_winname,windowname	endif	endmacromacro Change_Robust_Smoothing_Factor(sf)	variable sf	prompt sf, "Enter new smoothing factor. Typically sf = 1-3"	DoPrompt "Smoothing Factor",sf	wavestats/q matrix2	sf=(2*sf+1)^2/v_npnts	loess/r/ord=2/smth=(sf)/e=1/dest=matrix4 srcwave=matrix2	Differentiate/meth=0/DIM=0  matrix4	Differentiate/meth=0/DIM=1  matrix4	imagerotate/a=-45 matrix4	imageinterpolate/f={4,4} bilinear m_rotatedimageendmacromacro Change_Smoothing_Factor_Old(sf)	variable sf	prompt sf "Enter new smoothing factor for FORC analysis"	matrix4=forc(matrix2,p,q,sf);imagerotate/a=-45 matrix4;imageinterpolate/f={4,4} bilinear m_rotatedimageendmacro	function calc_forc_no_smooth()	Differentiate/DIM=0/meth=1  matrix2/D=W_DIF	Differentiate/DIM=1/meth=1  w_dif/D=W_DIF	imagerotate/a=-45 w_dif	imageinterpolate/f={4,4} bilinear m_rotatedimageendfunction Calc_Optimum_Smoothing()	variable start=1,stop=10,step=1		prompt start, "Enter Starting Smooth Factor"		DoPrompt "Smoothing Factor Start",start		prompt stop, "Enter Finishing Smooth Factor"		DoPrompt "Smoothing Factor Finish",stop		prompt step, "Enter Smooth Factor Increment"		DoPrompt "Smoothing Factor Increment",step	make/o/n=0 SmoothFactor,maxdev,sdev,sn	SmoothFactor=0	maxdev=0	sdev=0	sn=0	variable sf,p=0,rms1,rms2,sf2	wave matrix2	wave matrix4	//	newimage matrix4	display sdev vs smoothfactor	Label left "Standard Deviation of Residual"	Label bottom "Smooth factor"	ModifyGraph mode=4,marker=19,msize=2	for (sf=start;sf<=stop;sf+=step)		wavestats/q matrix2	sf2=(2*sf+1)^2/v_npnts		loess/ord=2/smth=(sf2)/e=1/dest=matrix4 srcwave=matrix2		wavestats/q matrix4		rms1=v_rms		matrix4-=matrix2		wavestats/q matrix4		redimension/n=(p+1) smoothfactor,maxdev,sdev,sn		SmoothFactor[p]=sf		Maxdev[p]=(abs(v_min) + abs(v_max))/2		sdev[p]=v_sdev		rms2=v_rms		sn[p]=20*log(rms1/rms2)		p+=1		doupdate	endfor	Duplicate/O sdev,sdev_smth;DelayUpdate	Loess/z=1/V=2/SMTH=0.25 factors={SmoothFactor}, srcWave= sdev_smth	differentiate sdev_smth	wavestats/q sdev_smth	display sdev_smth vs smoothfactor	ModifyGraph mode=4,marker=19,msize=2	Label left "Derivative of Standard Deviation of Residual"	Label bottom "Smooth factor"	print "Optimum Smooth Factor is",smoothfactor[v_minloc]	change_smoothing_factor(smoothfactor[v_minloc])endfunction Calc_Reversible_Ridge()	wave matrix4,ha	variable n=dimsize(matrix4,1)	make/o/n=(n) reversibleridge	Differentiate/DIM=0 /METH=1 matrix4/D=W_DIF	reversibleridge=w_dif[p][n-1-p]	reverse reversibleridge	wavestats/q reversibleridge	reversibleridge/=v_maxendfunction extend_matrix2()	variable n=dimsize(matrix2,1)	variable i,j	wave matrix2	for (i=0;i<n;i+=1)	j=n-1-i	matrix2[0,(j-1)][i]=matrix2[j][i]	endforendfunction add_colour_scale()	ColorScale/C/N=text0/F=0/A=RC/X=0.00/Y=0.00/E image=M_InterpolatedImageendmacro Display_Drift()	variable n=numpnts(driftfield)	if (driftfield[(n-1)]==0)		driftfield[(n-1)]=nan		driftmag[(n-1)]=nan	endif	wavestats/q driftfield	driftfield/=v_max	wavestats/q driftmag	driftmag/=v_max	display driftfield as "Drift Measurements"	append/r driftmag	ModifyGraph mode=4	ModifyGraph marker=19	ModifyGraph msize=1	ModifyGraph zero(bottom)=2	ModifyGraph nticks(left)=20	Label left "Field"	Label right "Mag"	Label bottom "Measurement Number"endmacromacro Display_Reversible_Ridge()	display reversibleridge vs ha as "Reversible Ridge"	ModifyGraph mode=4	ModifyGraph marker=19	ModifyGraph msize=1	ModifyGraph zero(bottom)=2	ModifyGraph nticks(left)=20	Label left "Normalised Reversible Ridge"	Label bottom "Hu (Oe)"endmacrofunction format_hysteresis(hc,hu,no)	variable hc,hu,no	variable ha,hb,step,i=0,j=0	step=(hu+hc)/no	make/o/n=((2*no),no) matrix1,matrix2	matrix1=nan	matrix2=nan	wave spinz1,field	variable k=0	for (i=0;i<no;i+=1)		ha=hu-i*step		for (j=0;j<(2*i+1);j+=1)		hb=ha+j*stepif ((hb-ha)/2 <= hc)		matrix1[(j+no-i-1)][i]=field[k]		matrix2[(j+no-i-1)][i]=spinz1[k]		k+=1endif		endfor		endfor			endfunction mask_curve(waveno)	variable waveno	if (waveno<0)		prompt waveno, "Enter reference number of FORC to be removed"		DoPrompt "FORC reference number",waveno	endif	variable flag=0,no	wave matrix2	if (mod(waveno,2)==0)		flag=1		doalert 0,"Reference number must be an odd number!"	endif	if (flag==0)		no=(waveno+1)/2 - 1		matrix2[][no]=nan	endifendfunction mask_area() : ButtonControl	wave matrix2	wave diff2	duplicate/o matrix2 matrix2save	duplicate/o diff2 diff2save	getmarquee left,top	variable l=ceil(v_left)	variable r=floor(v_right)	variable bo=floor(v_bottom)	variable to=ceil(v_top)	diff2[l,r][to,bo]=nan	matrix2[l,r][to,bo]=nanendfunction data_masking()execute "Masking_Panel()"endFunction maskbutton(ctrlName) : ButtonControl	String ctrlName	wave matrix2	wave diff2	duplicate/o matrix2 matrix2save	duplicate/o diff2 diff2save	getmarquee left,top	variable l=ceil(v_left)	variable r=floor(v_right)	variable bo=floor(v_bottom)	variable to=ceil(v_top)	diff2[l,r][to,bo]=nan	matrix2[l,r][to,bo]=nanEndFunction undobutton(ctrlName) : ButtonControl	String ctrlName	wave matrix2,diff2,matrix2save,diff2save	diff2=diff2save	matrix2=matrix2saveEndFunction displaydha(ctrlName) : ButtonControl	String ctrlName	wave matrix2	duplicate/o matrix2 diff2	Differentiate/meth=1/DIM=0  matrix2/D=diff2	checkdisplayed/a diff2	if (v_flag==0)		newimage diff2		ModifyImage diff2 ctab= {*,*,Rainbow,1}	endifEndFunction displayM(ctrlName) : ButtonControl	String ctrlName	wave matrix2	duplicate/o matrix2 diff2	checkdisplayed/a diff2	if (v_flag==0)		newimage diff2		ModifyImage diff2 ctab= {*,*,Rainbow,1}	endifEndFunction displaydhb(ctrlName) : ButtonControl	String ctrlName	wave matrix2	duplicate/o matrix2 diff2	Differentiate/meth=1/DIM=1  matrix2/D=diff2	checkdisplayed/a diff2	if (v_flag==0)		newimage diff2		ModifyImage diff2 ctab= {*,*,Rainbow,1}	endifEndFunction displaydhahb(ctrlName) : ButtonControl	String ctrlName	wave matrix2	duplicate/o matrix2 diff2	Differentiate/meth=1/DIM=1  matrix2/D=diff2	Differentiate/DIM=0/meth=1  matrix2/D=diff2	Differentiate/DIM=1/meth=1  diff2	checkdisplayed/a diff2	if (v_flag==0)		newimage diff2		ModifyImage diff2 ctab= {*,*,Rainbow,1}	endifEndWindow Masking_Panel() : Panel	PauseUpdate; Silent 1		// building window...	NewPanel /W=(843,67,1354,355)	SetDrawLayer UserBack	DrawText 18,50,"Choose Display Mode:"	DrawText 167,188,"Select area on raw data image."	DrawText 103,208," Press 'Mask' to remove that area from proc